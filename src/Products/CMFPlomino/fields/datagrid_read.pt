<div tal:define="
    field python:options['field'];
    fieldvalue python:options['fieldvalue']['rendered'];
    columns python:field.field_mapping.split(',');">
    <table>
        <tr>
            <th tal:repeat="col columns"
                tal:content="col">col</th>
        </tr>
        <tr tal:repeat="row fieldvalue">
            <td tal:repeat="cell row"
                tal:content="cell">cell</td>
        </tr>
    </table>
</div>